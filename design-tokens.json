{
  "colors": {
    "terminal": {
      "green": "#10B981",
      "amber": "#F59E0B",
      "red": "#EF4444",
      "cyan": "#06B6D4"
    },
    "fitness": {
      "primary": "#FB923C",
      "secondary": "#67E8F9",
      "success": "#34D399",
      "warning": "#FBBF24",
      "error": "#F87171"
    },
    "dark": {
      "background": "#000000",
      "surface": "#0A0A0A",
      "surfaceLight": "#1A1A1A",
      "text": "#F0F0F0",
      "textMuted": "#A0A0A0",
      "border": "#2A2A2A"
    },
    "light": {
      "background": "#FFFFFF",
      "surface": "#F5F5F5",
      "surfaceLight": "#E0E0E0",
      "text": "#1A1A1A",
      "textMuted": "#666666",
      "border": "#D0D0D0"
    }
  },

  "typography": {
    "fontFamily": {
      "heading": "'Inter', -apple-system, BlinkMacSystemFont, sans-serif",
      "body": "'Inter', -apple-system, BlinkMacSystemFont, sans-serif",
      "mono": "'JetBrains Mono', 'SF Mono', 'Consolas', monospace"
    },
    "fontSize": {
      "xs": "0.75rem",
      "sm": "0.875rem",
      "base": "1rem",
      "lg": "1.125rem",
      "xl": "1.25rem",
      "2xl": "1.5rem",
      "3xl": "2rem",
      "4xl": "2.5rem"
    },
    "fontWeight": {
      "normal": 400,
      "medium": 500,
      "semibold": 600,
      "bold": 700
    },
    "lineHeight": {
      "tight": 1.2,
      "normal": 1.5,
      "relaxed": 1.75
    }
  },

  "spacing": {
    "gridUnit": "4px",
    "scale": [4, 8, 12, 16, 24, 32, 40, 48, 64, 80, 96],
    "named": {
      "xxs": "4px",
      "xs": "8px",
      "s": "12px",
      "m": "16px",
      "l": "24px",
      "xl": "32px",
      "xxl": "48px",
      "xxxl": "64px"
    }
  },

  "borderRadius": {
    "sm": "0.25rem",
    "md": "0.5rem",
    "lg": "0.75rem",
    "xl": "1rem",
    "full": "9999px"
  },

  "shadows": {
    "sm": "0 1px 2px rgba(0, 0, 0, 0.05)",
    "md": "0 4px 6px rgba(0, 0, 0, 0.1)",
    "lg": "0 10px 15px rgba(0, 0, 0, 0.1)",
    "xl": "0 20px 25px rgba(0, 0, 0, 0.15)"
  },

  "transitions": {
    "fast": "150ms",
    "normal": "250ms",
    "slow": "350ms",
    "easing": "cubic-bezier(0.4, 0, 0.2, 1)",
    "spring": {
      "stiffness": 300,
      "damping": 20
    }
  },

  "breakpoints": {
    "mobile": "375px",
    "mobileWide": "390px",
    "tablet": "768px",
    "desktop": "1024px",
    "desktopMedium": "1440px",
    "wide": "1920px"
  },

  "accessibility": {
    "minTouchTarget": "44px",
    "minColorContrast": "4.5:1",
    "wcagLevel": "AA"
  },

  "performance": {
    "targets": {
      "firstContentfulPaint": "1800ms",
      "timeToInteractive": "3000ms",
      "initialLoad": "2000ms",
      "bundleSize": "250KB"
    },
    "fps": 60
  },

  "animations": {
    "cursorBlink": {
      "duration": "1s",
      "timingFunction": "step-end",
      "iterationCount": "infinite"
    },
    "fadeIn": {
      "duration": "0.3s",
      "timingFunction": "ease-out"
    },
    "slideUp": {
      "duration": "0.3s",
      "timingFunction": "ease-out"
    },
    "pulseTerminal": {
      "duration": "2s",
      "timingFunction": "ease-in-out",
      "iterationCount": "infinite"
    }
  },

  "fitnessSpecific": {
    "macroRings": {
      "strokeWidth": "12px",
      "gapSize": "8px",
      "animationDuration": "1000ms",
      "springPhysics": {
        "stiffness": 300,
        "damping": 20
      }
    },
    "progressIndicators": {
      "minHeight": "8px",
      "borderRadius": "4px",
      "animationDuration": "500ms"
    },
    "statsCards": {
      "minHeight": "120px",
      "padding": "24px",
      "borderRadius": "12px"
    }
  }
}
